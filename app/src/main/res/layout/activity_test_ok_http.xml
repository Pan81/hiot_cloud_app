<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".test.networktest.TestOkHttpActivity">

    <Button
        android:id="@+id/btn_okhttp_execute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="execute" />

    <Button
        android:id="@+id/btn_okhttp_enqueue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="enqueue" />

    <Button
        android:id="@+id/btn_okhttp_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登入" />

    <Button
        android:id="@+id/btn_okhttp_userinfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="用户信息" />

    <Button
        android:id="@+id/btn_okhttp_update_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="修改邮箱" />

</LinearLayout>